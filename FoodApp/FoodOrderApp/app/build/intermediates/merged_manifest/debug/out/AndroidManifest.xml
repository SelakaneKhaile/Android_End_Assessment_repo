<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.foodorderapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="24"
        android:targetSdkVersion="29" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:icon="@drawable/bike"
        android:label="@string/app_name"
        android:roundIcon="@drawable/bike"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.FoodOrderApp" >
        <activity
            android:name="com.android.foodorderapp.RegisterActivity"
            android:exported="false" >
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name="com.android.foodorderapp.MainActivity"
            android:exported="false" >
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity android:name="com.android.foodorderapp.OrderSucceessActivity" />
        <activity android:name="com.android.foodorderapp.PlaceYourOrderActivity" />
        <activity android:name="com.android.foodorderapp.RestaurantMenuActivity" />
        <activity android:name="com.android.foodorderapp.PasswordActivity" />
        <activity android:name="com.android.foodorderapp.LoginActivity" />
        <activity android:name="com.android.foodorderapp.LauncherActivity" />
        <activity android:name="com.android.foodorderapp.SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>